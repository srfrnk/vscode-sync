{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2000,
    "workbench.welcome.enabled": false,
    "git.confirmSync": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "git.enableSmartCommit": true,
    "eslint.enable": true,
    "macros": {
        "commentDown": [
            "editor.action.commentLine",
            "cursorDown"
        ],
        "duplicate": [
            "editor.action.clipboardCopyAction",
            "cursorRight",
            {
                "command": "type",
                "args": {
                    "text": " "
                }
            },
            "editor.action.clipboardPasteAction",
            "editor.action.formatSelection"
        ]
    },
    "extensions.recommendations": [
        "KnisterPeter.vscode-github",
        "PeterJausovec.vscode-docker",
        "alexnesnes.makeRunner",
        "Andreabbondanza.ignoregit",
        "alefragnani.project-manager",
        "bibhasdn.git-easy",
        "christian-kohler.npm-intellisense",
        "christian-kohler.path-intellisense",
        "cmstead.jsrefactor",
        "codezombiech.gitignore",
        "dbaeumer.vscode-eslint",
        "dbankier.vscode-quick-select",
        "donjayamanne.python",
        "eg2.vscode-npm-script",
        "formulahendry.code-runner",
        "jasonnutter.search-node-modules",
        "krizzdewizz.refactorix",
        "michelemelluso.gitignore",
        "mkloubert.vs-script-commands",
        "ntc.integrated-terminal-tasks",
        "redhat.java",
        "rogalmic.bash-debug",
        "stubailo.ignore-gitignore",
        "tht13.python",
        "waderyan.nodejs-extension-pack",
        "xabikos.JavaScriptSnippets",
        "donjayamanne.git-extension-pack",
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "felipecaputo.git-project-manager",
        "gabrielgrinberg.auto-run-command",
        "geddski.macros",
        "hasanali.gitignore-templates",
        "maelvalais.autoconf",
        "ryu1kn.annotator",
        "tomoki1207.pdf",
        "yzane.markdown-pdf",
        "ziyasal.vscode-open-in-github",
        "streetsidesoftware.code-spell-checker",
        "Hirse.vscode-ungit"
    ],
    "cSpell.userWords": [
        "polyfills",
        "codelyzer",
        "npmjs",
        "Injectable",
        "hammerjs",
        "datalab",
        "gcloud"
    ],
    "cSpell.enabledLanguageIds": [
        "c",
        "cpp",
        "csharp",
        "go",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "markdown",
        "php",
        "plaintext",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.ignorePaths":[
        "settings.json"
    ]
}